 Exec "Mountain" ps -k xlock & xlock -nolock -mode mountain -inroot

 Exec "Qix" ps -k xlock & xlock -nolock -mode qix -inroot

 Exec "Shape" ps -k xlock & xlock -nolock -mode shape -inroot

 Exec "Spermatozoid" ps -k xlock & xlock -nolock -mode swarm -inroot

 Exec "Spline" ps -k xlock & xlock -nolock -mode spline -inroot

 Exec "Star Trek" ps -k xlock & xlock -nolock -mode rock -inroot

 EndPopUp

